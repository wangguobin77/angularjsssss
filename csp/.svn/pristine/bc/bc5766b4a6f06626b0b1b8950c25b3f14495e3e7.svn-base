.pager{ 
    text-align: center;
}

.ng-pagination {
    font-family:'Microsoft YaHei';
    font-size:12px;
    color:#666;
}

.ng-pagination>ul {
    display:inline-block;
    margin:0;
    padding:0;
}

.ng-pagination>ul>li {
    margin-right:5px;
    /* border:1px solid #ad5257; */
    display:inline-block;
    cursor:pointer;
    border-radius:2px;
    /* background:#fff; */
    padding:6px 12px;
    vertical-align: middle;
    width: 10px;
    height: 30px;
    line-height: 30px;
    color: #828189;
}

.ng-pagination>ul>li:first-child {
    background: url(../../images/icon/icon_first.png) no-repeat center center;
}
.ng-pagination>ul>li:last-child {
    background: url(../../images/icon/icon_last.png) no-repeat center center;
}
.ng-pagination>ul>li:nth-child(2) {
    background: url(../../images/icon/icon_prev.png) no-repeat center center;
}
.ng-pagination>ul>li:nth-child(13) {
    background: url(../../images/icon/icon_next.png) no-repeat center center;
}
/* .ng-pagination>ul>li:hover */
.ng-pagination>ul>li.active {
    background: url(../../images/icon/icon_active.png) no-repeat center 80%;
    color:#212121;
}

.ng-pagination input {
    margin-left:2px;
    width:30px;
    border-radius:2px;
    border:1px solid #ddd;
    vertical-align:1px;
    text-align:center;
    padding:6px 3px;
}
